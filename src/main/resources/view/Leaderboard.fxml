<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="keltiga.controller.LeaderboardController">
    <children>

        <TableView fx:id="leaderboardTable" layoutX="100" layoutY="100" prefWidth="600" prefHeight="400">
            <columns>
                <TableColumn fx:id="usernameColumn" text="Username" prefWidth="300"/>
                <TableColumn fx:id="highScoreColumn" text="High Score" prefWidth="300"/>
            </columns>
        </TableView>


        <Button layoutX="350" layoutY="520" text="Next" onAction="#goToLevelSelection"/>
    </children>
</AnchorPane>

